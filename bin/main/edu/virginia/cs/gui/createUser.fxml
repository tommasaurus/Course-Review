<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="400.0" style="-fx-background-color: orange;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.virginia.cs.gui.ReviewController">
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="400.0">
         <children>
            <Text fill="WHITE" layoutX="40.0" layoutY="220.0" strokeType="OUTSIDE" strokeWidth="0.0" text="New Username">
               <font>
                  <Font name="Times New Roman" size="15.0" />
               </font>
            </Text>

            <TextField fx:id="newUsername" layoutX="150.0" layoutY="200.0" prefHeight="30.0" prefWidth="150.0" promptText="Username" style="-fx-border-color: black;">
               <font>
                  <Font name="Times New Roman" size="14.0" />
               </font>
            </TextField>

            <Text fill="WHITE" layoutX="40.0" layoutY="270.0" strokeType="OUTSIDE" strokeWidth="0.0" text="New Password">
               <font>
                  <Font name="Times New Roman" size="15.0" />
               </font>
            </Text>

            <PasswordField fx:id="newPassword1" layoutX="150.0" layoutY="250.0" prefHeight="30.0" prefWidth="150.0" promptText="Password" style="-fx-border-color: black;">
               <font>
                  <Font name="Times New Roman" size="14.0" />
               </font>
            </PasswordField>

            <Text fill="WHITE" layoutX="40.0" layoutY="320.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Retype Password">
               <font>
                  <Font name="Times New Roman" size="15.0" />
               </font>
            </Text>

            <PasswordField fx:id="newPassword2" layoutX="150.0" layoutY="300.0" prefHeight="30.0" prefWidth="150.0" promptText="Password" style="-fx-border-color: black;">
               <font>
                  <Font name="Times New Roman" size="14.0" />
               </font>
            </PasswordField>

            <Button fx:id="createUserButton" layoutX="150.0" layoutY="350.0" mnemonicParsing="false" onMouseClicked="#onClickCreateUserButton" prefWidth="100.0" text="Create User" visible="true">
               <font>
                  <Font size="15.0" />
               </font></Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
