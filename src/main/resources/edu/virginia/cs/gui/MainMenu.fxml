<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="400.0" style="-fx-background-color: orange;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.virginia.cs.gui.ReviewController">
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="400.0">
         <children>
            <Text fill="WHITE" layoutX="100" layoutY="40" strokeType="OUTSIDE" strokeWidth="0.0" text="Course Reviews" textAlignment="CENTER">
               <font>
                  <Font name="Times New Roman" size="32.0" />
               </font>
            </Text>
            <Button fx:id="SubmitReview" layoutX="120.0" layoutY="100.0" mnemonicParsing="false" onMouseClicked="#onClickSubmitReview" prefWidth="160.0" text="Submit A Review" visible="true">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Text fx:id="incorrectMainInput" fill="RED" layoutX="80.0" layoutY="400.0" text="INVALID INPUT" visible="false">
               <font>
                  <Font name="Times New Roman" size="35.0" />
               </font>
            </Text>
            <Button fx:id="SeeReview" layoutX="120.0" layoutY="200.0" mnemonicParsing="false" onMouseClicked="#onClickSeeReviewMain" prefWidth="160.0" text="See A Review" visible="true">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="logOut" layoutX="120.0" layoutY="300.0" mnemonicParsing="false" onMouseClicked="#onClicklogOutButton" prefWidth="160.0" text="Log Out" visible="true">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
